{
com.badlogic.gdx.graphics.g2d.BitmapFont: { 
	default-font: { file: font/default.fnt },
	android-font:{file: font/android.fnt},
	andy-font:{file: font/andy3.fnt},
	fennario-font:{file : font/fennario.fnt},
	thuphap-font:{file : font/thuphap.fnt},
	angry-font:{file : font/angry.fnt},
	angry2-font:{file : font/angry2.fnt},
},
com.badlogic.gdx.graphics.Color: {
	green: { a: 1, b: 0, g: 1, r: 0 },
	white: { a: 1, b: 1, g: 1, r: 1 },
	red: { a: 1, b: 0, g: 0, r: 1 },
	black: { a: 1, b: 0, g: 0, r: 0 },
	blue: {a: 1, b:1 , g: 1, r: 0 }
},
com.badlogic.gdx.scenes.scene2d.ui.Skin$TintedDrawable: {
